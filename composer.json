{
  "authors": [
    {
      "name": "robertkleinschuster",
      "email": "robert@kleinschuster.de",
      "homepage": "https://www.kleinschuster.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Pars\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ParsTest\\": "test"
    }
  },
  "description": "PSR-15 based Application Core",
  "license": "MIT",
  "minimum-stability": "stable",
  "name": "robertkleinschuster/pars",
  "require": {
    "php": ">=8.1",
    "ext-pdo": "*",
    "ext-intl": "*",
    "ext-tidy": "*",
    "ext-zend-opcache": "*",
    "psr/container": "1.1.2",
    "psr/log": "3.0.0",
    "psr/http-message": "1.0.1",
    "psr/http-factory": "1.0.1",
    "psr/http-server-middleware": "1.0.1",
    "psr/http-server-handler": "1.0.1",
    "psr/http-client": "1.0.1",
    "openswoole/ide-helper": "@dev",
    "mezzio/mezzio-swoole": "4.2.0",
    "mezzio/mezzio-router": "3.8.0",
    "mezzio/mezzio-fastroute": "3.5.0",
    "laminas/laminas-servicemanager": "3.14.0",
    "laminas/laminas-config-aggregator": "1.7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.13",
    "squizlabs/php_codesniffer": "3.6.2",
    "phpstan/phpstan": "1.4",
    "filp/whoops": "2.14.5"
  },
  "scripts": {
    "console": "bin/console",
    "phpcs": "phpcs",
    "phpcbf": "phpcbf",
    "phpstan": "phpstan analyse",
    "phpunit": "phpunit",
    "build": "@console build-entrypoints",
    "test": [
      "@phpcbf",
      "@phpcs",
      "@phpstan",
      "@phpunit"
    ]
  }
}
